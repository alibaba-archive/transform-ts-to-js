    "@iceworks/spec": "^1.0.0",
    "fs-extra": "^8.0.1",
    "eslint": "^6.8.0",
    "prettier": "^2.1.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^12.19.3",
    "@types/react": "^16.9.55",
    "glob": "^7.1.4",
    "jest": "^26.6.2",
    "react": "^17.0.1",
    "typescript": "^3.5.1"
